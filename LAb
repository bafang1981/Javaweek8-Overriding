class Virtualclass1{
 
   public void match()
   {
      System.out.println("Here is a video explaining the topic of the day");
   }
}
class lecturer extends Virtualclass1{
   
   public void play(){
      System.out.println("Erro");
   }
   public static void main( String args[]) {
      lecturer obj = new lecturer();
      //This will call the child class version of eat()
      obj.match();
   }
}
